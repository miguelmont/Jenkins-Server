  <description>The Jenkins Plugins Parent POM Project</description>
  <url>https://github.com/jenkinsci/beer-plugin</url>
  <inceptionYear>2016</inceptionYear>
  <scm>
    <connection>scm:git:https://github.com/jenkinsci/beer-plugin.git</connection>
    <developerConnection>scm:git:git@github.com:jenkinsci/beer-plugin.git</developerConnection>
